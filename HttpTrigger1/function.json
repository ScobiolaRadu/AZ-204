{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "myInputBlob",
      "type": "blob",
      "path": "incontainer/{name}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "myOutputBlob",
      "type": "blob",
      "path": "outcontainer/{name}",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
